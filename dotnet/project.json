{
  "name": "SuperSocket.ClientEngine",
  "version": "1.0.0-*",
  "buildOptions": {
    "emitEntryPoint": false,
    "compile": {
      "include": ["../**/*.cs"],
      "exclude": [
          "../dotnet/**/*.cs",
          "../**/Properties/*.cs",
          "../Properties/*.cs",
          "../**/*.Net20.cs",
          "../**/*.Silverlight.cs",
          "../**/*.Net40.cs",
          "../**/*.Net35.cs",
          "../**/*.WindowsPhone.cs",
          "../Protocol/**/*.cs",
          "../Common/DnsEndPoint.cs"
        ],
      "excludeFiles": [
          "../Common/ConnectAsyncExtension.Net.cs",
          "../Common/GenericDelegates.Func.cs",
          "../Common/GenericDelegates.Action.cs",
          "../Core/SslStream.cs",
          "../Core/NetworkStream.cs"
        ]
    }
  },
  "dependencies": {
      "SuperSocket.ProtoBase": "[1.7.0-beta6]"
  },
  "frameworks": {
    "netstandard1.3": {
      "buildOptions": {
        "define": [ "NETFX_CORE", "NETSTANDARD" ]
      },
      "dependencies": {
        "System.Linq": "4.1.0-rc2-*",
        "System.Net.Sockets": "4.1.0-rc2-*",
        "System.Runtime.Extensions": "4.1.0-rc2-*",
        "System.Runtime.InteropServices": "4.1.0-rc2-*",
        "System.Security.Cryptography.X509Certificates": "4.1.0-rc2-*",
        "System.Net.Security": "4.0.0-rc2-*",
        "System.Net.NameResolution": "4.0.0-rc2-*"
      }
    }
  }
}
